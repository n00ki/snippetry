{
  "type": "module",
  "name": "snippetry",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:push:remote": "NODE_ENV=production drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:remote": "NODE_ENV=production drizzle-kit migrate",
    "db:seed": "vite-node --options.transformMode.ssr='/.*/' src/lib/db/seed",
    "db:drop": "drizzle-kit drop",
    "db:clear": "vite-node --options.transformMode.ssr='/.*/' src/lib/db/clear",
    "db:studio": "drizzle-kit studio",
    "db:studio:remote": "NODE_ENV=production drizzle-kit studio",
    "test": "pnpm test:integration && pnpm test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin prettier-plugin-svelte . --check . && eslint .",
    "format": "prettier --write  --plugin prettier-plugin-svelte .",
    "test:integration": "pnpm db:clear && NODE_ENV=test playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@playwright/test": "^1.46.0",
    "@sveltejs/adapter-vercel": "^5.4.3",
    "@sveltejs/kit": "^2.5.21",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@types/eslint": "^9.6.0",
    "@types/node": "^22.0.0",
    "autoprefixer": "^10.4.20",
    "bits-ui": "^0.21.13",
    "clsx": "^2.1.1",
    "cssnano": "^7.0.5",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.24.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.43.0",
    "formsnap": "^1.0.1",
    "globals": "^15.8.0",
    "postcss": "^8.4.41",
    "postcss-load-config": "^5.0.3",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "radix-icons-svelte": "^1.2.1",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.5",
    "svelte-french-toast": "^1.2.0",
    "sveltekit-superforms": "^2.16.1",
    "tailwind-merge": "^2.4.0",
    "tailwind-variants": "^0.2.1",
    "tailwindcss": "^3.4.9",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.4.0",
    "vite-node": "^2.0.5",
    "vitest": "^2.0.5",
    "zod": "^3.23.8"
  },
  "dependencies": {
    "@libsql/client": "^0.8.0",
    "@tiptap/core": "^2.5.9",
    "@tiptap/extension-code-block-lowlight": "^2.5.9",
    "@tiptap/extension-document": "^2.5.9",
    "@tiptap/extension-paragraph": "^2.5.9",
    "@tiptap/extension-text": "^2.5.9",
    "drizzle-orm": "^0.33.0",
    "fathom-client": "^3.7.2",
    "logsnag": "^1.0.0",
    "lowlight": "^3.1.0",
    "nanoid": "^5.0.7",
    "shiki": "^1.12.1",
    "svelte-inview": "^4.0.2",
    "sveltekit-flash-message": "^2.4.4"
  }
}
